{
  "name": "64",
  "version": "0.2.0",
  "description": "High performance Base64 encoding and decoding for Node.js using SIMD acceleration",
  "author": "Lovell Fuller <npm@lovell.info>",
  "homepage": "https://github.com/lovell/64",
  "license": "Apache-2.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/lovell/64"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "test": "semistandard && cpplint && ava && node test/bench && prebuild-ci"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "binding.gyp",
    "src/**"
  ],
  "keywords": [
    "base64",
    "simd",
    "decode",
    "encode"
  ],
  "dependencies": {
    "nan": "^2.14.2",
    "prebuild-install": "^5.3.5"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "benchmark": "^2.1.4",
    "cc": "^2.0.1",
    "prebuild": "^10.0.1",
    "prebuild-ci": "^3.1.0",
    "semistandard": "^14.2.3"
  },
  "cc": {
    "ignore": [
      "node_modules",
      "src/include",
      "src/lib"
    ]
  },
  "semistandard": {
    "ignore": [
      "test/bench-results.js"
    ]
  },
  "ava": {
    "files": [
      "test/unit.js"
    ]
  }
}
